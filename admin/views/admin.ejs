<html><!DOCTYPE html>
<head>
	<title> Admins</title>
</head>
<body>
<h1>Admin List</h1>
<table border="1" cellpadding="7" cellspacing="7">
	<tr>
    <th width="50px">No</th>
    <th>Admin Id</th>
    <th>Last Name</th>
    <th>First Name</th>
  </tr>
  <% if(admin.length){
    for(var i = 0;i < admin.length;i++) { %>
  <tr>
    <td><%=(i+1)%></td>
    <td><%=admin[i].admin_id%></td>
    <td><%=admin[i].last_name%></td>
    <td><%=admin[i].first_name%></td>
  </tr>
  <% }
  }else{ %>

  <tr>
    <td colspan="3">No user</td>
  </tr>
<% } %>
</table>

</body>
</html>
